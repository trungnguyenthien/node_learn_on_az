{
    "manifest_version": 2,
    "name": "My Extension",
    "version": "1.0",
    "description": "A simple Chrome extension",
    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    },
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "main.html"
    },
    "permissions": [
        "tabs"
    ],
    "content_scripts": [
        {
            "matches": [
                "http://*/*"
            ],
            "js": [
                "jquery-3.6.0.min.js",
                "axios.min.js"
            ]
        }
    ]
}